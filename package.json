{
  "name": "clarity-impact-finance",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@vercel/analytics": "^1.5.0",
    "airtable": "^0.12.2",
    "emailjs-com": "^3.2.0",
    "react": "^18.2.0",
    "react-calendly": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-pdf": "^7.7.3",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "smoothscroll-polyfill": "^0.4.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false node src/scripts/generate-sitemap.js && node src/scripts/generate-robots.js && REACT_APP_VERCEL=true REACT_APP_BUILD_TIME=$(date +%s) react-scripts build",
    "vercel-build": "CI=false npm install && REACT_APP_VERCEL=true react-scripts build",
    "cloudflare-build": "CI=false npm install --legacy-peer-deps && node src/scripts/generate-sitemap.js && node src/scripts/generate-robots.js && react-scripts build",
    "cloudflare-deploy": "sh ./build-for-cloudflare.sh",
    "generate-seo": "node src/scripts/generate-sitemap.js && node src/scripts/generate-robots.js",
    "generate-webp": "node src/scripts/generate-webp.js",
    "optimize-images": "npm run generate-webp && npm run check-images",
    "check-images": "node src/scripts/optimize-images.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "chalk": "^5.4.1",
    "file-loader": "^6.2.0",
    "png-to-ico": "^2.1.8",
    "sharp": "^0.33.5",
    "svg2png": "^4.1.1",
    "yocto-queue": "^1.2.1"
  }
}
